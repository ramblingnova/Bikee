<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF">

    <ImageView
        android:id="@+id/lister_side_menu_lister_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon" />

    <TextView
        android:id="@+id/lister_side_menu_fragment_see_my_bicycle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_lister_image"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon"
        android:text="내 자전거 보기" />

    <TextView
        android:id="@+id/lister_side_menu_fragment_register_smart_lock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_fragment_see_my_bicycle"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon"
        android:text="내 스마트락 등록" />

    <TextView
        android:id="@+id/lister_side_menu_fragment_receive_payment_information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_fragment_register_smart_lock"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon"
        android:text="대금수령정보" />

    <TextView
        android:id="@+id/lister_side_menu_evaluation_bicycle_script"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_fragment_receive_payment_information"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon"
        android:text="내평가보기" />

    <TextView
        android:id="@+id/lister_side_menu_authentication_information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_evaluation_bicycle_script"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon"
        android:text="인증" />

    <TextView
        android:id="@+id/lister_side_menu_push_alarm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_authentication_information"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon"
        android:text="푸시알림" />

    <Switch
        android:id="@+id/lister_side_menu_push_alarm_switch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_authentication_information"
        android:layout_toRightOf="@id/lister_side_menu_push_alarm" />

    <TextView
        android:id="@+id/lister_side_menu_input_inquiry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_push_alarm"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon"
        android:text="고객센터" />

    <TextView
        android:id="@+id/lister_side_menu_version_information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/lister_side_menu_input_inquiry"
        android:layout_centerHorizontal="true"
        android:src="@drawable/temp_icon"
        android:text="버전정보" />

    <Button
        android:id="@+id/lister_side_menu_change_mode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:text="Renter모드 전환" />
</RelativeLayout>