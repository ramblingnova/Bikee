<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF00FFFF"
    android:padding="10dp"
    tools:context="com.example.tacademy.bikee.SearchResultFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/fragment_search_result_temp_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <!-- TODO : DELME -->
            <TextView
                android:id="@+id/search_result_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="검색 결과 프래그먼트" />

            <Switch
                android:id="@+id/search_bar_switch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/search_result_text" />
        </RelativeLayout>

        <FrameLayout
            android:id="@+id/container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/fragment_search_result_temp_layout"></FrameLayout>
    </RelativeLayout>
</FrameLayout>