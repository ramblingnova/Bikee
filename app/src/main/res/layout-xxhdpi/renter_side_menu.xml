<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF"
    android:orientation="vertical"
    android:focusable="true"
    android:clickable="true">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.337">

        <ImageView
            android:id="@+id/renter_side_menu_renter_image_image_view"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:src="@mipmap/ic_launcher" />

        <TextView
            android:id="@+id/renter_side_menu_member_name_text_view"
            style="?attr/textAppearanceListItemSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/renter_side_menu_renter_image_image_view"
            android:layout_centerHorizontal="true"
            android:text="@string/renter_side_menu_member_name_text_view_string" />

        <TextView
            android:id="@+id/renter_side_menu_mail_address_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/renter_side_menu_member_name_text_view"
            android:layout_centerHorizontal="true"
            android:text="@string/renter_side_menu_mail_address_text_view_string" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.163"
        android:paddingLeft="30dp"
        android:paddingTop="15dp">

        <TextView
            android:id="@+id/renter_side_menu_fragment_register_card_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_margin="3dp"
            android:src="@drawable/temp_icon"
            android:text="@string/renter_side_menu_register_card_text_view_string" />

        <TextView
            android:id="@+id/renter_side_menu_evaluation_bicycle_script_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/renter_side_menu_fragment_register_card_text_view"
            android:layout_margin="3dp"
            android:src="@drawable/temp_icon"
            android:text="@string/renter_side_menu_evaluation_bicycle_script_text_view_string" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.43"
        android:paddingLeft="30dp"
        android:paddingTop="15dp">

        <RelativeLayout
            android:id="@+id/renter_side_menu_authentication_information_text_view_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/renter_side_menu_authentication_information_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_margin="3dp"
                android:src="@drawable/temp_icon"
                android:text="@string/renter_side_menu_authentication_information_text_view_string" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/renter_side_menu_push_alarm_text_view_check_box_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/renter_side_menu_authentication_information_text_view_layout">

            <TextView
                android:id="@+id/renter_side_menu_push_alarm_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_margin="3dp"
                android:src="@drawable/temp_icon"
                android:text="@string/renter_side_menu_push_alarm_text_view_string" />

            <CheckBox
                android:id="@+id/renter_side_menu_push_alarm_switch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_margin="3dp"
                android:layout_toRightOf="@id/renter_side_menu_push_alarm_text_view"
                android:background="@drawable/push_alarm_check_selector"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/renter_side_menu_input_inquiry_text_view_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/renter_side_menu_push_alarm_text_view_check_box_layout">

            <TextView
                android:id="@+id/renter_side_menu_input_inquiry_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerHorizontal="true"
                android:layout_margin="3dp"
                android:src="@drawable/temp_icon"
                android:text="@string/renter_side_menu_input_inquiry_text_view_string" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/renter_side_menu_version_information_text_view_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/renter_side_menu_input_inquiry_text_view_layout">

            <TextView
                android:id="@+id/renter_side_menu_version_information_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_margin="3dp"
                android:src="@drawable/temp_icon"
                android:text="@string/renter_side_menu_version_information_text_view_string" />
        </RelativeLayout>
    </RelativeLayout>

    <Button
        android:id="@+id/renter_side_menu_change_mode_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_weight="0.07"
        android:text="@string/renter_side_menu_change_mode_button_string" />
</LinearLayout>