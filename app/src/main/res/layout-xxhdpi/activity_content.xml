                        android:layout_width="26dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/detailpage_icon6" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="11dp"
                        android:layout_marginStart="11dp"
                        android:layout_toEndOf="@id/activity_content_bicycle_rental_period_total_icon_image_view"
                        android:layout_toRightOf="@id/activity_content_bicycle_rental_period_total_icon_image_view"
                        android:text="총 시간"
                        android:textColor="@color/bikeeGray"
                        android:textSize="16dp" />

                    <TextView
                        android:id="@+id/activity_content_bicycle_rental_period_total_time_text_view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:textColor="@color/bikeeBlue"
                        android:textSize="16dp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_alignParentBottom="true"
                        android:background="@color/bikeeLightGray" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/activity_content_bicycle_rental_price_layout"
                    android:layout_width="match_parent"
                    android:layout_height="91dp"
                    android:layout_below="@id/activity_content_bicycle_rental_period_total_time_layout"
                    android:layout_marginBottom="32dp"
                    android:background="@drawable/detailpage_back_img1"
                    android:paddingEnd="16dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingStart="16dp">

                    <TextView
                        android:id="@+id/activity_content_bicycle_rental_price_text_view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:textColor="@color/bikeeBlue"
                        android:textSize="22dp" />

                    <ImageView
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="11dp"
                        android:layout_marginRight="11dp"
                        android:layout_toLeftOf="@id/activity_content_bicycle_rental_price_text_view"
                        android:layout_toStartOf="@id/activity_content_bicycle_rental_price_text_view"
                        android:src="@drawable/detailpage_icon8" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/activity_content_bicycle_comment_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/activity_content_bicycle_rental_price_layout"
                    android:background="@drawable/detailpage_back_img2"
                    android:paddingBottom="26dp"
                    android:paddingEnd="16dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingStart="16dp"
                    android:paddingTop="25dp"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/activity_content_bicycle_comment_renter_picture_image_view"
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/noneimage" />

                    <TextView
                        android:id="@+id/activity_content_bicycle_comment_renter_name_text_view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@id/activity_content_bicycle_comment_renter_picture_image_view"
                        android:layout_marginLeft="11dp"
                        android:layout_marginStart="11dp"
                        android:layout_toEndOf="@id/activity_content_bicycle_comment_renter_picture_image_view"
                        android:layout_toRightOf="@id/activity_content_bicycle_comment_renter_picture_image_view"
                        android:textColor="@color/bikeeBlue"
                        android:textSize="12dp" />

                    <TextView
                        android:id="@+id/activity_content_bicycle_comment_create_date_text_view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@id/activity_content_bicycle_comment_renter_name_text_view"
                        android:layout_alignStart="@id/activity_content_bicycle_comment_renter_name_text_view"
                        android:layout_below="@id/activity_content_bicycle_comment_renter_name_text_view"
                        android:layout_marginTop="9dp"
                        android:textColor="@color/bikeeLightGray"
                        android:textSize="10dp" />

                    <TextView
                        android:id="@+id/activity_content_bicycle_comment_body_text_view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@id/activity_content_bicycle_comment_renter_name_text_view"
                        android:layout_alignStart="@id/activity_content_bicycle_comment_renter_name_text_view"
                        android:layout_below="@id/activity_content_bicycle_comment_create_date_text_view"
                        android:layout_marginTop="24dp"
                        android:textColor="@color/bikeeGray"
                        android:textSize="12dp" />

                    <RatingBar
                        android:id="@+id/activity_content_bicycle_comment_rating_bar"
                        style="@style/BikeeRatingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@id/activity_content_bicycle_comment_renter_name_text_view"
                        android:layout_alignStart="@id/activity_content_bicycle_comment_renter_name_text_view"
                        android:layout_below="@id/activity_content_bicycle_comment_body_text_view"
                        android:layout_marginTop="12dp"
                        android:isIndicator="true" />

                    <Button
                        android:id="@+id/activity_content_bicycle_comment_button"
                        android:layout_width="72dp"
                        android:layout_height="30dp"
                        android:layout_below="@id/activity_content_bicycle_comment_rating_bar"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/detailpage_icon_more" />
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/activity_content_bottom_buttons_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/activity_content_bicycle_comment_layout"
                    android:layout_marginBottom="26dp"
                    android:layout_marginTop="42dp"
                    android:orientation="horizontal"
                    android:paddingEnd="16dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingStart="16dp">

                    <Button
                        android:id="@+id/activity_content_bottom_buttons_left_button"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/detailpage_button1"
                        android:text="뒤로가기"
                        android:textColor="@color/bikeeWhite" />

                    <Button
                        android:id="@+id/activity_content_bottom_buttons_right_button"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_weight="0.5"
                        android:background="@drawable/detailpage_button2"
                        android:textColor="@color/bikeeWhite"
                        android:visibility="gone" />
                </LinearLayout>
            </RelativeLayout>
        </ScrollView>

        <View
            android:layout_width="match_parent"
            android:layout_height="4dp"
            android:layout_alignParentTop="true"
            android:background="@drawable/shadow" />
    </RelativeLayout>
</android.support.design.widget.CoordinatorLayout>