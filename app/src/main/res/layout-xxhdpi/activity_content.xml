                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/activity_content_bicycle_height_layout"
                    android:background="@color/bikeeWhite"
                    android:paddingEnd="16dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingStart="16dp">

                    <ImageView
                        android:id="@+id/activity_content_bicycle_components_icon_image_view"
                        android:layout_width="26dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/detailpage_icon3" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="11dp"
                        android:layout_marginStart="11dp"
                        android:layout_toEndOf="@id/activity_content_bicycle_components_icon_image_view"
                        android:layout_toRightOf="@id/activity_content_bicycle_components_icon_image_view"
