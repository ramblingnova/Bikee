<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="com.bigtion.bikee.common.content.ContentActivity">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <android.support.v7.widget.Toolbar
            android:id="@+id/activity_content_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            app:contentInsetLeft="0dp"
            app:contentInsetStart="0dp"
            app:popupTheme="@style/AppTheme.PopupOverlay" />
    </android.support.design.widget.AppBarLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bikeeBackground">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <FrameLayout
                    android:id="@+id/activity_content_bicycle_pictures_user_information_layout"
                    android:layout_width="match_parent"
                    android:layout_height="307dp"
                    android:background="@color/bikeeWhite"
                    android:paddingBottom="29dp">

                    <RelativeLayout
                        android:id="@+id/activity_content_bicycle_pictures_layout"
                        android:layout_width="match_parent"
                        android:layout_height="248dp"
                        android:layout_gravity="top">

                        <android.support.v4.view.ViewPager
                            android:id="@+id/activity_content_bicycle_pictures_view_pager"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/activity_content_user_information_layout"
                        android:layout_width="match_parent"
                        android:layout_height="58dp"
                        android:layout_gravity="bottom"
                        android:paddingEnd="16dp"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:paddingStart="16dp">

                        <ImageView
                            android:id="@+id/activity_content_user_information_user_picture_image_view"
                            android:layout_width="58dp"
                            android:layout_height="58dp"
                            android:layout_alignParentBottom="true"
                            android:scaleType="fitXY"
                            android:src="@drawable/noneimage" />

                        <TextView
                            android:id="@+id/activity_content_user_information_user_name_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_marginBottom="14dp"
                            android:layout_marginLeft="8dp"
                            android:layout_marginStart="8dp"
                            android:layout_toEndOf="@id/activity_content_user_information_user_picture_image_view"
                            android:layout_toRightOf="@id/activity_content_user_information_user_picture_image_view"
                            android:textColor="@color/bikeeBlack"
                            android:textSize="12dp" />

                        <Button
                            android:id="@+id/activity_content_user_information_chatting_button"
                            android:layout_width="27dp"
                            android:layout_height="27dp"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:background="@drawable/detailpage_icon_massege"
                            android:scaleType="fitXY" />

                        <Button
                            android:id="@+id/activity_content_user_information_call_button"
                            android:layout_width="27dp"
                            android:layout_height="27dp"
                            android:layout_alignBottom="@id/activity_content_user_information_chatting_button"
                            android:layout_marginEnd="10dp"
                            android:layout_marginRight="10dp"
                            android:layout_toLeftOf="@id/activity_content_user_information_chatting_button"
                            android:layout_toStartOf="@id/activity_content_user_information_chatting_button"
                            android:background="@drawable/detailpage_icon_call"
                            android:scaleType="fitXY" />
                    </RelativeLayout>
                </FrameLayout>

                <RelativeLayout
                    android:id="@+id/activity_content_bicycle_title_introduction_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/activity_content_bicycle_pictures_user_information_layout"
                    android:background="@color/bikeeWhite"
                    android:paddingEnd="16dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingStart="16dp">

                    <TextView
                        android:id="@+id/activity_content_bicycle_title_text_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/bikeeBlack"
                        android:textSize="16dp" />

                    <TextView
                        android:id="@+id/activity_content_bicycle_introduction_text_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/activity_content_bicycle_title_text_view"
