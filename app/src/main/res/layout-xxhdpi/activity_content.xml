                    android:layout_height="wrap_content"
                    android:layout_below="@+id/activity_content_bicycle_location_map_layout"
                    android:background="@color/bikeeWhite"
                    android:paddingEnd="16dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingStart="16dp"
                    android:paddingTop="21dp"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/activity_content_bicycle_price_decision_icon_image_view"
                        android:layout_width="20dp"
                        android:layout_height="26dp"
                        android:src="@drawable/detailpage_icon8_1" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="11dp"
                        android:layout_marginStart="11dp"
                        android:layout_toEndOf="@id/activity_content_bicycle_price_decision_icon_image_view"
                        android:layout_toRightOf="@id/activity_content_bicycle_price_decision_icon_image_view"
                        android:text="대여요금"
                        android:textColor="@color/bikeeGray"
                        android:textSize="16dp" />

                    <RelativeLayout
                        android:id="@+id/activity_content_bicycle_price_decision_price_per_hour_layout"
                        android:layout_width="159dp"
                        android:layout_height="37dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true">

                        <ImageView
                            android:id="@+id/activity_content_bicycle_price_decision_price_per_hour_icon_image_view"
                            android:layout_width="17dp"
                            android:layout_height="17dp"
                            android:layout_marginBottom="20dp"
                            android:src="@drawable/detailpage_charge_icon1" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="8dp"
                            android:layout_marginStart="8dp"
                            android:layout_toEndOf="@id/activity_content_bicycle_price_decision_price_per_hour_icon_image_view"
                            android:layout_toRightOf="@id/activity_content_bicycle_price_decision_price_per_hour_icon_image_view"
                            android:text="시간당"
                            android:textColor="@color/bikeeGray"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/activity_content_bicycle_price_decision_price_per_hour_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:textColor="@color/bikeeBlue"
                            android:textSize="16dp" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:layout_alignParentBottom="true"
                            android:background="@color/bikeeLightGray" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/activity_content_bicycle_price_decision_price_per_day_layout"
                        android:layout_width="159dp"
                        android:layout_height="58dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/activity_content_bicycle_price_decision_price_per_hour_layout"
                        android:paddingTop="21dp">

                        <ImageView
                            android:id="@+id/activity_content_bicycle_price_decision_price_per_day_icon_image_view"
                            android:layout_width="17dp"
                            android:layout_height="17dp"
                            android:layout_marginBottom="20dp"
                            android:src="@drawable/detailpage_charge_icon2" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="8dp"
                            android:layout_marginStart="8dp"
                            android:layout_toEndOf="@id/activity_content_bicycle_price_decision_price_per_day_icon_image_view"
                            android:layout_toRightOf="@id/activity_content_bicycle_price_decision_price_per_day_icon_image_view"
                            android:text="일당"
                            android:textColor="@color/bikeeGray"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/activity_content_bicycle_price_decision_price_per_day_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:textColor="@color/bikeeBlue"
                            android:textSize="16dp" />

                        <View
                            android:id="@+id/activity_content_"
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:layout_alignParentBottom="true"
                            android:background="@color/bikeeLightGray" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/activity_content_bicycle_price_decision_price_per_month_layout"
