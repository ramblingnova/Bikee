                    android:paddingEnd="16dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingStart="16dp">

                    <ImageView
                        android:id="@+id/activity_content_bicycle_type_icon_image_view"
                        android:layout_width="26dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/detailpage_icon1" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="11dp"
                        android:layout_marginStart="11dp"
                        android:layout_toEndOf="@id/activity_content_bicycle_type_icon_image_view"
                        android:layout_toRightOf="@id/activity_content_bicycle_type_icon_image_view"
                        android:text="종류"
                        android:textColor="@color/bikeeGray"
                        android:textSize="16dp" />

                    <TextView
                        android:id="@+id/activity_content_bicycle_type_text_view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:textColor="@color/bikeeBlue"
                        android:textSize="16dp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_alignParentBottom="true"
                        android:background="@color/bikeeLightGray" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/activity_content_bicycle_height_layout"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:layout_below="@id/activity_content_bicycle_type_layout"
